class Solution(object):
    def maximumOddBinaryNumber(self, s):
        size= len(s)
        count_0=0
        count_1=0

        for i in range(size):
            if s[i] == '1':
                count_1+=1
            else:
                count_0+=1
        res=""
        for i in range(count_1-1):
            res+='1'
        for j in range(count_1,size):
            res+='0'
        res+='1'
        return res

        
